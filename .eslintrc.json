{
  "extends": "airbnb/base",
  "rules": {
    "func-names": ["off"],
    "comma-dangle": ["off"],
    "no-param-reassign": ["off"],
    "wrap-iife": ["error", "inside"],
    "arrow-body-style": ["error", "always"],
    "quotes": ["error", "double", { "allowTemplateLiterals": true }],
    "no-unused-vars": ["error", { "args": "none" }]
  }
}
